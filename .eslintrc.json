{
    "plugins": [ "jasmine" ],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "jasmine/no-focused-tests": "error",
        "jasmine/missing-expect": "warn"
    },
    "env": {
        "es6": true,
        "node": true,
        "jasmine": true
    },
    "extends": "standard"
}
